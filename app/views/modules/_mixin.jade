mixin list(classUl, ...items)
    ul(class=classUl)
        each item in items
            li.nav__element
                a(href="#").nav__element-item= item

mixin link(title, href, text)
    a(href=href ? href : '#', class="nav__element-item", title!= title ? title : text)&attributes(attributes)!= text ? text : title

mixin multiMenu(obj, key)
    - each item,index in obj
        - var active  = ''
        - if(index === key) active='active nav__element-parent'
        li.nav__element(class=active)
            +link(item.title, item.href, item.text)
            - if(typeof(item.subMenu) !== 'undefined') {
                ul.sub-nav.list-unstyled
                    +multiMenu(item.subMenu, item.subActive)
            - }

